    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run HEADLESS",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": ["--", "--headless", ]
        },
        {
            "name": "Run GUI",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File with profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": [
                "-o",
                "/tmp/tmp.prof",
                "${file}"
            ]
        },
        {
            "name": "Generate Experiment Plots (back_and_forth)",
            "type": "debugpy",
            "request": "launch",
            //"program": "main.py",
            //"program": "pattern_experiment.py",
            "program": "back_and_forth_experiment.py",
            "console": "integratedTerminal"
        }
    ]
}